# DataFed username
DATAFED_USERNAME=your_username

# DataFed password
DATAFED_PASSWORD=your_password

# Watch directory where microscope saves files
WATCH_DIRECTORY=path/to/microscope/files

# Log file path
LOG_FILE=path/to/log/file.log

# DataFed repository ID
DATAFED_REPO_ID=your_repo_id

# Default tags for DataFed records
DEFAULT_TAGS=tag1,tag2,tag3

# DataFed home directory
DATAFED_HOME=path/to/datafed/home

# DataFed API endpoint
DATAFED_API_ENDPOINT=https://api.datafed.io/v1

# DataFed API key
DATAFED_API_KEY=your_api_key

# DataFed API secret
DATAFED_API_SECRET=your_api_secret





